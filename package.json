{
  "name": "genshin-impact-theme-pack",
  "displayName": "Genshin Impact Theme Pack",
  "description": "A set of themes of your favorite Teyvat companions.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Themes"
  ],
  "activationEvents": [
    "onTheme"
  ],
  "main": "./out/extension.ts",
  "contributes": {
    "themes": [
      {
        "label": "Raiden Shogun Theme",
        "uiTheme": "vs-dark",
        "path": "./themes/Raiden Shogun.json"
      }
    ],
    "commands": [
      {
        "command": "genshin-impact-theme-pack.selectTheme",
        "title": "Select Theme"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.92.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0"
  }
}
